{
  "compilerOptions": {
    "baseUrl": "src",
    "paths": {
      "@/*": ["*"],
      "@modules/*": ["modules/*"],
      "@components/*": ["components/*"],
      "@hooks/*": ["hooks/*"],
      "@contexts/*": ["contexts/*"],
      "@utils/*": ["utils/*"],
      "@config/*": ["config/*"],
      "@services/*": ["services/*"]
    }
  },
  "include": ["src"]
}
